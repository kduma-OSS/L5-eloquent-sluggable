{
  "name": "kduma/eloquent-sluggable",
  "description": "Eases using and generating slugs Laravel Eloquent models.",
  "keywords": ["laravel", "eloquent", "slug"],
  "license": "MIT",
  "authors": [
    {
      "name": "Krystian Duma",
      "email": "git@kduma.tk"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "5.*"
  },
  "autoload": {
    "psr-4": {
      "KDuma\\Eloquent\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master-v1": "1.x-dev",
      "dev-master": "2.x-dev"
    }
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "^5.5"
  }
}
